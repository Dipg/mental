/* Editar # de Relevancia */
.tag[data-ref*="ur"] {
	visibility: hidden important;
    opacity: 0.5;
	} 
.tag[data-ref*=in] {
		visibility: hidden important;
	} 

    .tag[data-ref*="a"]{
        visibility: hidden;
        position: relative;
        margin-right:-10px !important;
        margin-left:-1px !important;
}
    
    .tag[data-ref*="inmediato"]::before{
        visibility: visible;
        content: "!!!" !important;
        margin-left:-5px !important;
        margin-right:-10px !important;
        background-color:red !important;
        color: rgb(255, 255, 255) !important;
        opacity: 0.7 !important;
        border-radius: 10px !important;
        padding-left: 5px !important;
        padding-right: 5px !important;
}

.tag[data-ref*="urgente"]{
    visibility: hidden;
    position: relative;
    margin-right:-10px !important;
    margin-left:-1px !important;
}
        .tag[data-ref*="urgente"]::before{
            visibility: visible;
            content: "!!" !important;
            margin-left:-5px !important;
            margin-right:-10px !important;
            background-color:rgb(246, 160, 0) !important;
            color: rgb(255, 255, 255) !important;
            opacity: 0.7 !important;
            border-radius: 10px !important;
            padding-left: 5px !important;
            padding-right: 5px !important;
        }



        .tag[data-ref*="hacer"]{
            visibility: hidden;
            position: relative;
            margin-right:-10px !important;
            margin-left:-1px !important;
        }
                .tag[data-ref*="hacer"]::before{
                    visibility: visible;
                    content: "!" !important;
                    margin-left:-5px !important;
                    margin-right:-10px !important;
                    background-color:rgb(0, 255, 0) !important;
                    color: rgb(255, 255, 255) !important;
                    opacity: 0.7 !important;
                    border-radius: 10px !important;
                    padding-left: 5px !important;
                    padding-right: 5px !important;
                }